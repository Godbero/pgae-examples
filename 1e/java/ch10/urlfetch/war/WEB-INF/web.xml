<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  <servlet>
    <servlet-name>urlconnection</servlet-name>
    <servlet-class>urlfetch.URLConnectionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>urlconnection</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>urlconnection</servlet-name>
    <url-pattern>/urlconnection</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>urlfetch</servlet-name>
    <servlet-class>urlfetch.URLFetchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>urlfetch</servlet-name>
    <url-pattern>/urlfetch</url-pattern>
  </servlet-mapping>
</web-app>
